{
  "npmScope": "dinivas",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "api": {
      "tags": ["scope:api"]
    },
    "api-interfaces": {
      "tags": ["scope:shared"]
    },
    "common-ui": {
      "tags": ["scope:client"]
    },
    "dinivas": {
      "tags": ["scope:client"]
    },
    "dinivas-e2e": {
      "tags": ["scope:client"],
      "implicitDependencies": ["dinivas"]
    },
    "ng-material": {
      "tags": ["scope:client"]
    },
    "packer-worker": {
      "tags": ["scope:api"]
    },
    "terraform-worker": {
      "tags": ["scope:api"]
    }
  }
}
