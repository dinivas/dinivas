{
  "name": "dinivas",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "nx": "nx",
    "start": "ng serve",
    "build": "run-s build:api build:dinivas:docker build:packer-worker build:terraform-worker",
    "build:api": "ng build api --prod",
    "build:dinivas": "ng build --prod --stats-json",
    "build:dinivas:docker": "ng build --configuration=docker --stats-json",
    "build:packer-worker": "ng build packer-worker  --prod",
    "build:terraform-worker": "ng build terraform-worker --prod",
    "build:docker": "run-s docker:build:api docker:build:packer-worker docker:build:terraform-worker docker:build:dinivas",
    "stats:dinivas": "webpack-bundle-analyzer dist/apps/dinivas/stats.json",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "affected:ci:build": "nx affected:build  --configuration=docker --base=HEAD~1 --head=HEAD --plain",
    "affected:ci:docker": "nx affected --target=docker --base=HEAD~1 --head=HEAD --plain",
    "compodoc": "npx compodoc",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "docker:build:api": "docker build -t dinivas/api -f ./apps/api/Dockerfile .",
    "docker:build:dinivas": "docker image build -t dinivas/dinivas -f ./apps/dinivas/Dockerfile .",
    "docker:build:packer-worker": "docker build -t dinivas/packer-worker -f ./apps/packer-worker/Dockerfile .",
    "docker:build:terraform-worker": "docker build -t dinivas/terraform-worker -f ./apps/terraform-worker/Dockerfile .",
    "docker:dev:ps": "docker-compose -p dinivas -f docker-compose.dev.yml ps",
    "docker:dev:up": "docker-compose -p dinivas -f docker-compose.dev.yml up -d",
    "docker:dev:down": "docker-compose -p dinivas -f docker-compose.dev.yml down",
    "docker:ps": "docker-compose -p dinivas ps",
    "docker:up": "docker-compose -p dinivas up -d",
    "docker:down": "docker-compose -p dinivas down"
  },
